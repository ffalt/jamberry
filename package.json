{
  "name": "jamberry",
  "version": "0.4.20",
  "author": "ffalt",
  "license": "MIT",
  "description": "a html5 jamserve web player",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:prod": "ng serve --aot --configuration production",
    "build": "ng build",
    "build:AOT": "ng build --aot",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "clean": "rimraf dist",
    "release": "commit-and-tag-version -a",
    "update:angular": "ng update @angular/cli @angular/core",
    "build:changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 1",
    "build:releaselog": "conventional-changelog -p angular -r 2 -o RELEASELOG.md",
    "build:prebuild": "npm run clean && npm run build:version",
    "build:version": "node ./build-pre.js",
    "bundle-report": "npm run build:stats && webpack-bundle-analyzer --mode static dist/stats.json",
    "build:stats": "ng build --named-chunks --configuration production --stats-json --output-hashing=none",
    "build:prod": "ng build --verbose --aot --configuration production --base-href \"/\"",
    "build:develop": "npm run build:prebuild && ng build --progress --base-href \"/\"",
    "build:deploy": "npm run build:prebuild && npm run build:prod",
    "test:watch": "jest --watch"
  },
  "engines": {
    "node": ">=16"
  },
  "private": true,
  "standard-version": {
    "scripts": {
      "postbump": "node ./build-pre.js && git add ./src/environments/version.ts"
    }
  },
  "dependencies": {
    "@angular/animations": "15.1.0",
    "@angular/cdk": "15.1.0",
    "@angular/common": "15.1.0",
    "@angular/compiler": "15.1.0",
    "@angular/core": "15.1.0",
    "@angular/forms": "15.1.0",
    "@angular/platform-browser": "15.1.0",
    "@angular/platform-browser-dynamic": "15.1.0",
    "@angular/platform-server": "15.1.0",
    "@angular/router": "15.1.0",
    "core-js": "3.27.1",
    "hammerjs": "2.0.8",
    "mousetrap": "1.6.5",
    "ngx-image-cropper": "6.3.2",
    "rxjs": "7.6.0",
    "soundmanager2": "2.97.20170602",
    "tslib": "2.4.1",
    "zone.js": "0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "15.1.1",
    "@angular-eslint/builder": "15.1.0",
    "@angular-eslint/eslint-plugin": "15.1.0",
    "@angular-eslint/eslint-plugin-template": "15.1.0",
    "@angular-eslint/schematics": "15.1.0",
    "@angular-eslint/template-parser": "15.1.0",
    "@angular/cli": "15.1.1",
    "@angular/compiler-cli": "15.1.0",
    "@angular/language-service": "15.1.0",
    "@types/hammerjs": "2.0.41",
    "@types/jest": "29.2.5",
    "@types/mousetrap": "1.6.11",
    "@types/node": "18.11.18",
    "@typescript-eslint/eslint-plugin": "5.48.1",
    "@typescript-eslint/parser": "5.48.1",
    "conventional-changelog-cli": "2.2.2",
    "eslint": "8.31.0",
    "eslint-plugin-angular": "4.1.0",
    "eslint-plugin-import": "2.27.4",
    "eslint-plugin-jsdoc": "39.6.4",
    "eslint-plugin-no-null": "latest",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-rxjs": "5.0.2",
    "eslint-plugin-unicorn": "latest",
    "jest": "28.1.3",
    "jest-preset-angular": "12.2.3",
    "ng-mocks": "14.5.2",
    "postcss": "8.4.21",
    "rimraf": "4.0.4",
    "commit-and-tag-version": "10.1.0",
    "ts-node": "10.9.1",
    "typescript": "4.9.4",
    "webpack-bundle-analyzer": "4.7.0"
  }
}
