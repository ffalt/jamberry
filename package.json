{
  "name": "jamberry",
  "version": "0.2.27-alpha",
  "author": "ffalt",
  "license": "MIT",
  "description": "a html5 jamserve web player",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:prod": "ng serve --aot --prod",
    "build": "ng build",
    "build:AOT": "ng build --aot",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "eslint": "eslint  ./src --ext .ts",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "clean": "rimraf dist",
    "update:angular": "ng update @angular/cli @angular/core",
    "build:changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "build:prebuild": "npm run clean && node ./build-pre.js",
    "bundle-report": "npm run build:stats && webpack-bundle-analyzer --mode static dist/stats.json",
    "build:stats": "ng build --named-chunks --prod --stats-json --output-hashing=none",
    "build:prod": "npm run build:prebuild && ng build --aot --prod --base-href \".\"",
    "build:develop": "npm run build:prebuild && ng build --progress --base-href \"/\"",
    "build:deploy": "npm run build:prebuild && ng build --aot --prod --base-href \".\"",
    "test:watch": "jest --watch"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "9.1.4",
    "@angular/cdk": "9.2.2",
    "@angular/common": "9.1.4",
    "@angular/compiler": "9.1.4",
    "@angular/core": "9.1.4",
    "@angular/forms": "9.1.4",
    "@angular/platform-browser": "9.1.4",
    "@angular/platform-browser-dynamic": "9.1.4",
    "@angular/platform-server": "9.1.4",
    "@angular/router": "9.1.4",
    "core-js": "3.6.5",
    "hammerjs": "2.0.8",
    "mousetrap": "1.6.5",
    "ngx-image-cropper": "3.1.8",
    "rxjs": "6.5.5",
    "soundmanager2": "2.97.20170602",
    "tslib": "1.11.2",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.901.4",
    "@angular/cli": "9.1.4",
    "@angular/compiler-cli": "9.1.4",
    "@angular/language-service": "9.1.4",
    "@briebug/jest-schematic": "2.1.1",
    "@types/hammerjs": "2.0.36",
    "@types/jest": "25.2.1",
    "@types/mousetrap": "1.6.3",
    "@types/node": "13.13.5",
    "@typescript-eslint/eslint-plugin": "2.31.0",
    "@typescript-eslint/parser": "2.31.0",
    "codelyzer": "5.2.2",
    "conventional-changelog-cli": "2.0.31",
    "eslint": "6.8.0",
    "eslint-plugin-angular": "4.0.1",
    "jest": "25.5.4",
    "jest-preset-angular": "8.2.0",
    "ng-mocks": "9.2.0",
    "rifraf": "2.0.3",
    "rxjs-tslint-rules": "4.31.0",
    "ts-node": "8.10.1",
    "tslint": "6.1.2",
    "tslint-etc": "1.10.1",
    "typescript": "3.8.3",
    "webpack-bundle-analyzer": "3.7.0"
  }
}
