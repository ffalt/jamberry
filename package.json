{
  "name": "jamberry",
  "version": "0.2.13-alpha",
  "author": "ffalt",
  "license": "MIT",
  "description": "a html5 jamserve web player",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:prod": "ng serve --aot --prod",
    "build": "ng build",
    "build:AOT": "ng build --aot",
    "test": "jest",
    "test:coverage": "jest --coverage true",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "clean": "rimraf dist",
    "update:angular": "ng update --next=true --force @angular/cli @angular/core",
    "release": "deplokay -c deplokay.json",
    "build:changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "build:prebuild": "npm run clean && node ./build-pre.js",
    "bundle-report": "npm run build:stats && webpack-bundle-analyzer --mode static dist/stats.json",
    "build:stats": "ng build --named-chunks --prod --stats-json --output-hashing=none",
    "build:electron": "npm run build:prebuild && ng build --prod --progress --base-href \"./\" --deleteOutputPath=true --outputPath=./electron/dist/berry/",
    "build:prod": "npm run build:prebuild && ng build --aot --prod --progress --base-href \"/\"",
    "build:develop": "npm run build:prebuild && ng build --progress --base-href \"/\"",
    "build:deploy": "npm run build:prebuild && ng build --aot --prod --base-href \"/\"",
    "test:watch": "jest --watch"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "9.0.0-rc.4",
    "@angular/cdk": "9.0.0-next.0",
    "@angular/common": "9.0.0-rc.4",
    "@angular/compiler": "9.0.0-rc.4",
    "@angular/core": "9.0.0-rc.4",
    "@angular/forms": "9.0.0-rc.4",
    "@angular/platform-browser": "9.0.0-rc.4",
    "@angular/platform-browser-dynamic": "9.0.0-rc.4",
    "@angular/platform-server": "9.0.0-rc.4",
    "@angular/router": "9.0.0-rc.4",
    "core-js": "3.4.7",
    "hammerjs": "2.0.8",
    "mousetrap": "1.6.3",
    "ngx-image-cropper": "2.0.2",
    "rxjs": "6.5.3",
    "soundmanager2": "2.97.20170602",
    "tslib": "1.10.0",
    "zone.js": "0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.900.0-rc.4",
    "@angular/cli": "9.0.0-rc.4",
    "@angular/compiler-cli": "9.0.0-rc.4",
    "@angular/language-service": "9.0.0-rc.4",
    "@briebug/jest-schematic": "2.1.1",
    "@types/hammerjs": "2.0.36",
    "@types/jest": "24.0.23",
    "@types/mousetrap": "1.6.3",
    "@types/node": "12.12.14",
    "codelyzer": "5.2.0",
    "conventional-changelog-cli": "^2.0.28",
    "deplokay": "0.0.11",
    "jest": "24.9.0",
    "jest-preset-angular": "8.0.0",
    "ng-mocks": "^8.1.0",
    "rifraf": "2.0.3",
    "rxjs-tslint-rules": "4.27.1",
    "ts-node": "8.5.4",
    "tslint": "5.20.1",
    "tslint-etc": "1.9.2",
    "typescript": "3.6.4",
    "webpack-bundle-analyzer": "3.6.0"
  }
}
