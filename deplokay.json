{
	"$schema": "node_modules/deplokay/config-schema.json",
	"id": "berry-deploy",
	"source": {
		"local": {
			"path": "."
		}
	},
	"publish": {
		"archive": {
			"path": "./local/archives"
		},
		"branch": {
			"branch": "releases"
		}
	},
	"build": {
		"npm": {
			"cmd_name": "build:deploy",
			"folder_names": [
				"dist"
			]
		}
	},
	"env": {
		"NODE_ENV": "development"
	}
}
